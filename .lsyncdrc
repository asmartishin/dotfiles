settings {
    logfile    = "/tmp/lsyncd.log",
    statusFile = "/tmp/lsyncd.status",
    nodaemon   = true,
}

sync {
    default.rsyncssh,
    source="/Users/user/trunk",
    host="ip",
    targetdir="/home/user/trunk",
    rsync = {
        rsh = "/usr/bin/ssh -l user -i /Users/user/.ssh/id_rsa",
        archive = true,
        compress = false,
        whole_file = false,
        binary = "/usr/local/bin/rsync"
    }
}
